
{
    "name": "Dev container: api-security-ait-dtu-dk",
    "dockerComposeFile": "docker-compose.yaml",
    "service": "api-security-ait-dtu-dk",
    "workspaceFolder": "/mnt/project",
    "customizations": {
        "vscode": {
            "settings": {
                "python.defaultInterpreterPath": "/usr/src/project/app/venv/bin/python"
            },
            "extensions": [
                "ms-python.python",
                "rogalmic.bash-debug"
            ]
        }
    },
    "postCreateCommand": "bash .devcontainer/postCreateCommand.sh"
}
