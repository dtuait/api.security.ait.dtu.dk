

{
    "name": "Dev container: my x-api.security.ait.dtu.dk",
    "dockerComposeFile": "my-development-docker-compose.yaml",
    "service": "xx-api-security-ait-dtu-dk-app-main",
    "workspaceFolder": "/usr/src/project",
    "customizations": {
        "vscode": {
            "settings": {
                "python.defaultInterpreterPath": "/usr/src/venvs/app-main/bin/python"
            },
            "extensions": [
                "GitHub.copilot",
                "ms-python.vscode-pylance",
                "ms-python.debugpy",
                "ms-python.python",
                "stuart.unique-window-colors"
            ]
        }
    },
    "postCreateCommand": "bash .devcontainer/postCreateCommand.sh",
    "postStartCommand": "bash .devcontainer/postStartCommand.sh"
}

